  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.41",
    "@typescript-eslint/parser": "^5.27.1",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.16.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jest": "^26.4.6",
    "jest": "^28.1.0",
